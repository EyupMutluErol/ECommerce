{
  "ConnectionStrings": {
    "DefaultConnection": "YOUR_DEFAULT_CONNECTION_STRING",
    "IdentityConnection": "YOUR_IDENTITY_CONNECTION_STRING"
  },

  "Payment": {
    "Iyzico": {
      "ApiKey": "YOUR_API_KEY",
      "SecretKey": "YOUR_SECRET_KEY",
      "BaseUrl": "https://sandbox-api.iyzipay.com"
    }
  },

  "EmailSender": {
